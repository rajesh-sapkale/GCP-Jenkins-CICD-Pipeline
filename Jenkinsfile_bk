pipeline {
  agent {docker 'maven'}
  stages {
    stage('checkout') {
      steps {
        git 'https://github.com/rajesh-sapkale/spring-petclinic.git'
      }
      }
    stage('build') {
      steps {
        sh 'mvn clean package'
        junit '**/target/surefire-reports/TEST-*.xml'
      }
    }
  }
}
